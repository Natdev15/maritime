{
  "name": "maritime-container-dashboard",
  "version": "1.0.0",
  "description": "Maritime container tracking dashboard with Brotli compression and SQLite storage",
  "main": "esp32-encoder.js",
  "scripts": {
    "start": "node esp32-encoder.js",
    "dev": "nodemon esp32-encoder.js",
    "encoder": "node esp32-encoder.js",
    "decoder": "node esp32-cbor-decoder.js",
    "test": "node test-complete-pipeline.js",
    "test-complete": "node test-complete-pipeline.js",
    "test-bulk": "node bulk-esp32-test.js",
    "test-single": "node test-complete-pipeline.js individual",
    "docker-encoder": "docker-compose up -d esp32-encoder",
    "docker-decoder": "docker-compose up -d esp32-decoder",
    "docker-logs": "docker-compose logs -f",
    "docker-down": "docker-compose down",
    "deploy-local": "docker-compose up -d esp32-encoder",
    "deploy-vm": "docker-compose up -d esp32-decoder"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cbor": "^10.0.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "maritime",
    "container",
    "tracking",
    "brotli",
    "sqlite"
  ],
  "author": "",
  "license": "ISC"
}
